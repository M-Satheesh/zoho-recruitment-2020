public static void main(String[] args) 
{
    Scanner sc = new Scanner(System.in);
    System.out.println("Enter the no:: ");
		int x = sc.nextInt();
    for(int i=x;i>0;i--)
 {
    for(int j=x;j>i;j--)
   System.out.print(j);
   for(int j=i;j>0;j--)
   System.out.print(i);
   System.out.println();
  }
}
